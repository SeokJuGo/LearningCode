    "nums_pileup = 1  # 벌집의 개수, 1개부터 시작\n",
    "cnt = 1\n",
    "while n > nums_pileup :\n",
    "    nums_pileup += 6 * cnt  # 벌집이 6의 배수로 증가\n",
    "    cnt += 1\n",
    "print(cnt)"
